{
  "comment": "Ansible manageged. Local modifactions may overwritten.",
  "policies": {
{% if full_ca_cert_path is defined and full_ca_cert_path | length > 0 %}
      "Certificates": {
          "ImportEnterpriseRoots": true,
          "Install": [
              "{{ full_ca_cert_path }}"
          ]
      },
{% endif %}
    "DisablePocket": true,
    "SSLVersionMin": "tls1.2",
    "Homepage": {
    "URL": "https://lab-awx.bodomos.lan:8043/#/login",
    "Locked": true,
    "Additional": [],
    "StartPage": "previous-session"
    },
    "SupportMenu": {
    "Title": "Selfe Service",
    "URL": "https://lab-awx.bodomos.lan:8043/#/login",
    "AccessKey": "S"
    },
    "RequestedLocales": ["de", "en-US"],
    "ExtensionSettings": {
{% if firefox_allowed_to_install_extensions is defined and not firefox_allowed_to_install_extensions %}
      "*": {
        "blocked_install_message": "Install userbased addons is not allowed in the lab environment.",
        "install_sources": ["about:addons","https://addons.mozilla.org/"],
        "installation_mode": "blocked",
        "allowed_types": ["extension"]
      },
{% endif %}
      "uBlock0@raymondhill.net": {
        "installation_mode": "force_installed",
        "install_url": "https://addons.mozilla.org/firefox/downloads/latest/ublock-origin/latest.xpi"
      },
      "plasma-browser-integration@kde.org": {
        "installation_mode": "force_installed",
        "install_url": "https://addons.mozilla.org/firefox/downloads/file/3859385/plasma_integration-1.8.1-fx.xpi"
      },
      "langpack-de@firefox.mozilla.org": {
        "installation_mode": "force_installed",
        "install_url": "https://addons.mozilla.org/firefox/downloads/file/3935106/deutsch_de_language_pack-99.0.1buildid20220411.174855-fx.xpi"
      }
    },
    "ExtensionUpdate": true
  }
}
